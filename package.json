{
  "name": "Buckutt",
  "main": "app.js",
  "description": "Systeme de paiement electronique",
  "dependencies": {
    "bluebird": "^2.5.3",
    "body-parser": "1.3.1",
    "express": "^4.7.2",
    "forever": "^0.11.1",
    "inflection": "^1.5.2",
    "mariasql": "^0.1.21",
    "method-override": "^2.1.2",
    "moment": "^2.8.3",
    "morgan": "^1.2.2",
    "mysql": "^2.4.1",
    "nconf": "^0.6.9",
    "orm": "^2.1.17",
